{
    "manifestVersion": 1,
    "id": "f5-tasks",
    "name": "F5 BIG-IP Local Traffic Manager (Unofficial)",
    "version": "0.3.4",
    "publisher": "ReubenCabrera",
    "targets": [
        {
            "id": "Microsoft.VisualStudio.Services"
        }
    ],    
    "description": "Unofficial tools for working with the F5 BIG-IP Local Traffic Manager load balancer.  Disable pool members before a deployment, and re-enable them when complete.",
    "categories": [
        "Build and release"
    ],
    "tags": ["f5", "ltm", "load balancer"],
    "icons": {
        "default": "images/extension-icon-128.png"
    },
    "branding": {
        "color": "#444444",
        "theme": "dark"
    },
    "screenshots": [
        { "path": "screenshots/screen01.png" },
        { "path": "screenshots/screen02.png" },
        { "path": "screenshots/screen03.png" }
    ],
    "content": {
        "details": {
            "path": "overview.md"
        }
    },
    "files": [
        { "path": "Tasks" }
    ],
    "contributions": [
        {
            "id": "ChangeF5PoolStatus",
            "type": "ms.vss-distributed-task.task",
            "targets": [
                "ms.vss-distributed-task.tasks"
            ],
            "properties": {
                "name": "Tasks/F5-ChangePoolStatus"
            }
        }
    ]
}